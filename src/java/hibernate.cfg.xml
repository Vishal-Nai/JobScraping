<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.connection.driver_class">com.microsoft.sqlserver.jdbc.SQLServerDriver</property>
    <property name="hibernate.connection.url">jdbc:sqlserver://;databaseName=job</property>
    <property name="hibernate.connection.username">sa</property>
    <property name="hibernate.connection.password">vishal</property>
    <mapping class="model.Login"></mapping>
    <mapping class="model.UserReg"></mapping>
    <mapping class="model.State"></mapping>
    <mapping class="model.City"></mapping>
  </session-factory>
</hibernate-configuration>
